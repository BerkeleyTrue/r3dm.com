.main-app
  height 100%
  width 100%
  padding-bottom 60px
  overflow hidden

  .logo-container
    height 100%
    pointer-events none
    position absolute
    text-align center
    width 100%
    z-index 10

    &:before
      content ''
      display inline-block
      height 100%
      vertical-align middle

    .logo
      svg
        height 30vh
        left 35vw
        position absolute
        top 40vh
        width 30vw

.v-center
  display inline-block
  width 100%
  vertical-align middle

.first-con_bot
  text-align center
  p
    font-size 25px
    font-weight 600
    padding 0 10px
    padding-top 25vh
