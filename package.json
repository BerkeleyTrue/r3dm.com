{
  "name": "r3dm.com",
  "version": "1.0.0",
  "description": "The R3DM, an isomorphic page",
  "main": "server/server.js",
  "browser": "client.js",
  "scripts": {
    "start": "babel-node server/server.js",
    "lint": "eslint --ext=.js,.jsx .",
    "test": "npm run lint",
    "prestart": "npm run build-bundle && npm run build-css",
    "build-bundle": "browserify -t babelify -g envify -t uglifyify client.js -o public/js/bundle.js",
    "build-css": "stylus --use kouto-swiss --include-css -p ./components/app.styl | cleancss -o public/css/main.css"
  },
  "engines": {
    "iojs": "1.6.x"
  },
  "repository": {
    "type": "git",
    "url": "https://berkeleytrue@bitbucket.org/bigr3dm/frontface.git"
  },
  "keywords": [
    "rx",
    "r3dm",
    "react",
    "flux",
    "isomorphic",
    "keystone"
  ],
  "author": [
    "Berkeley Martinez <berkeley@r3dm.com> (http://the.r3dm.com/)",
    "Harry Moreno <harry@r3dm.com> (http://the.r3dm.com/)"
  ],
  "license": "None",
  "devDependencies": {
    "browser-sync": "^2.1.1",
    "babel-eslint": "^3.1.8",
    "eslint": "^0.21.0",
    "eslint-plugin-react": "^2.2.0",
    "gulp": "^3.8.10",
    "gulp-concat": "^2.4.1",
    "gulp-imagemin": "^1.1.0",
    "gulp-minify-css": "^0.3.10",
    "gulp-nodemon": "^2.0.3",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^0.6.6",
    "gulp-ruby-sass": "^0.7.1",
    "gulp-stylus": "^1.3.4",
    "gulp-util": "^3.0.1",
    "gulp-watch": "^3.0.0",
    "lodash": "^3.3.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.1.1",
    "yargs": "^1.3.2"
  },
  "dependencies": {
    "agenda": "^0.6.26",
    "babel": "^5.2.17",
    "babelify": "^6.1.0",
    "body-parser": "^1.6.5",
    "browserify": "^8.1.3",
    "classnames": "^2.1.1",
    "clean-css": "^3.2.9",
    "compression": "^1.0.11",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^0.7.0",
    "cookie-parser": "^1.3.2",
    "cookie-session": "^1.0.2",
    "debug": "^2.0.0",
    "dotenv": "^0.4.0",
    "envify": "^3.2.0",
    "errorhandler": "^1.1.1",
    "express": "^4.8.5",
    "express-session": "^1.10.0",
    "express-state": "^1.2.0",
    "fetchr": "^0.4.0",
    "helmet": "^0.5.0",
    "jade": "^1.7.0",
    "keystone": "0.3.8",
    "kouto-swiss": "^0.11.11",
    "mandrill-api": "^1.0.41",
    "moment": "^2.9.0",
    "mongoose": "^4.0.2",
    "morgan": "^1.5.0",
    "multer": "^0.1.7",
    "newrelic": "^1.13.2",
    "object.assign": "^2.0.1",
    "q": "^1.1.2",
    "react": "^0.13.3",
    "react-imageloader": "^1.1.2",
    "react-inlinesvg": "^0.4.1",
    "react-loadermixin": "^1.0.4",
    "react-router": "^0.13.3",
    "react-tween-state": "0.0.4",
    "rx": "^2.3.14",
    "serve-favicon": "^2.1.1",
    "serve-static": "^1.5.3",
    "stylus": "^0.51.1",
    "thundercats": "^2.0.0-alpha.7",
    "uglifyify": "^3.0.1"
  }
}
