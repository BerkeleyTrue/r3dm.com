{
  "name": "r3dm.com",
  "version": "1.0.0",
  "description": "The R3DM, an isomorphic page",
  "main": "server/server.js",
  "browser": "client.js",
  "scripts": {
    "start": "babel-node server/server.js",
    "lint": "eslint --ext=.js,.jsx .",
    "test": "npm run lint",
    "prestart": "npm run build-bundle && npm run build-css",
    "build-bundle": "browserify -t babelify -g envify -t uglifyify client.js -o public/js/bundle.js",
    "build-css": "stylus --use kouto-swiss --include-css -p ./components/app.styl | cleancss -o public/css/main.css"
  },
  "engines": {
    "iojs": "1.6.x"
  },
  "repository": {
    "type": "git",
    "url": "https://berkeleytrue@bitbucket.org/bigr3dm/frontface.git"
  },
  "keywords": [
    "rx",
    "r3dm",
    "react",
    "flux",
    "isomorphic",
    "keystone"
  ],
  "author": [
    "Berkeley Martinez <berkeley@r3dm.com> (http://the.r3dm.com/)",
    "Harry Moreno <harry@r3dm.com> (http://the.r3dm.com/)"
  ],
  "license": "None",
  "devDependencies": {
    "browser-sync": "^2.1.1",
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.8.2",
    "gulp": "^3.8.10",
    "gulp-concat": "^2.4.1",
    "gulp-imagemin": "^4.1.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-nodemon": "^2.0.3",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.0",
    "gulp-ruby-sass": "^3.0.0",
    "gulp-stylus": "^2.7.0",
    "gulp-util": "^3.0.1",
    "gulp-watch": "^5.0.0",
    "lodash": "^4.17.10",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.0",
    "yargs": "^11.0.0"
  },
  "dependencies": {
    "accepts": "^1.2.7",
    "agenda": "^1.0.3",
    "babel": "^6.23.0",
    "babelify": "^8.0.0",
    "body-parser": "^1.6.5",
    "browserify": "^16.2.2",
    "classnames": "^2.1.1",
    "clean-css": "^4.1.11",
    "compression": "^1.0.11",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.3.2",
    "cookie-session": "^1.0.2",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "envify": "^4.1.0",
    "errorhandler": "^1.1.1",
    "express": "^4.8.5",
    "express-session": "^1.10.0",
    "express-state": "^1.2.0",
    "fetchr": "^0.5.37",
    "helmet": "^3.12.1",
    "jade": "^1.7.0",
    "keystone": "0.3.22",
    "kouto-swiss": "^1.1.0",
    "mandrill-api": "^1.0.41",
    "moment": "^2.9.0",
    "mongoose": "^5.1.2",
    "morgan": "^1.5.0",
    "multer": "^1.3.0",
    "newrelic": "^4.1.1",
    "object.assign": "^4.1.0",
    "q": "^1.1.2",
    "react": "^16.3.2",
    "react-imageloader": "^3.0.0",
    "react-ink": "^6.2.0",
    "react-inlinesvg": "^0.8.1",
    "react-loadermixin": "^1.0.4",
    "react-router": "^4.2.0",
    "react-tween-state": "0.1.5",
    "rx": "^4.1.0",
    "serve-favicon": "^2.1.1",
    "serve-static": "^1.5.3",
    "stylus": "^0.54.5",
    "thundercats": "3.1.0",
    "uglifyify": "^5.0.0"
  }
}
